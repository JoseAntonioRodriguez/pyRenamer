<gconfschemafile>
	<schemalist>
		<schema>
			<key>/schemas/apps/pyrenamer/root_dir</key>
			<applyto>/apps/pyrenamer/root_dir</applyto>
			<owner>pyrenamer</owner>
			<type>string</type>
			<default></default>
			<locale name="C">
				<short>Default root dir</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/active_dir</key>
			<applyto>/apps/pyrenamer/active_dir</applyto>
			<owner>pyrenamer</owner>
			<type>string</type>
			<default></default>
			<locale name="C">
				<short>Default active dir</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/window_maximized</key>
			<applyto>/apps/pyrenamer/window_maximized</applyto>
			<owner>pyrenamer</owner>
			<type>bool</type>
			<default>0</default>
			<locale name="C">
				<short>Is the window maximized?</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/pane_position</key>
			<applyto>/apps/pyrenamer/pane_position</applyto>
			<owner>pyrenamer</owner>
			<type>int</type>
			<default>250</default>
			<locale name="C">
				<short>Position of the pane separator</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/window_width</key>
			<applyto>/apps/pyrenamer/window_width</applyto>
			<owner>pyrenamer</owner>
			<type>int</type>
			<default>500</default>
			<locale name="C">
				<short>Window width</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/window_height</key>
			<applyto>/apps/pyrenamer/window_height</applyto>
			<owner>pyrenamer</owner>
			<type>int</type>
			<default>500</default>
			<locale name="C">
				<short>Window height</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/window_posx</key>
			<applyto>/apps/pyrenamer/window_posx</applyto>
			<owner>pyrenamer</owner>
			<type>int</type>
			<default>0</default>
			<locale name="C">
				<short>X position of the window</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/window_posy</key>
			<applyto>/apps/pyrenamer/window_posy</applyto>
			<owner>pyrenamer</owner>
			<type>int</type>
			<default>0</default>
			<locale name="C">
				<short>Y position of the window</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/options_shown</key>
			<applyto>/apps/pyrenamer/options_shown</applyto>
			<owner>pyrenamer</owner>
			<type>bool</type>
			<default>0</default>
			<locale name="C">
				<short>Display the options pane</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/options_filedir</key>
			<applyto>/apps/pyrenamer/options_filedir</applyto>
			<owner>pyrenamer</owner>
			<type>int</type>
			<default>0</default>
			<locale name="C">
				<short>Rename files or directories</short>
				<long>0: Rename files / 1: Rename directories / 2: Rename files and directories</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/keepext</key>
			<applyto>/apps/pyrenamer/keepext</applyto>
			<owner>pyrenamer</owner>
			<type>bool</type>
			<default>0</default>
			<locale name="C">
				<short>Keep files extensions</short>
				<long></long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/pyrenamer/autopreview</key>
			<applyto>/apps/pyrenamer/autopreview</applyto>
			<owner>pyrenamer</owner>
			<type>bool</type>
			<default>0</default>
			<locale name="C">
				<short>Auto preview changes</short>
				<long></long>
			</locale>
		</schema>
	</schemalist>
</gconfschemafile>
